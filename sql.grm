$OREXPRESSION = $COMPARAISON | $EXPRESSION $OROP $EXPRESSION 
$EXPRESSION = $OREXPRESSION | $NOTOP $EXPRESSION
$COMPARAISON = $TERM $COMPOP $TERM 
$COMPOP = " > " | " = " | " < " 
$OROP = " or " 
$NOTOP = " not "  
$TERM = $NUMBER | $STRING | $QUOTE $TERM $QUOTE 
$NUMBER = [0-9]+ 
$STRING = [a-z]+ 
$QUOTE = "\"" 
