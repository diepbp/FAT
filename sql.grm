$QUERY = $HEADER $NAME $WHERE $OREXPRESSION
$WHERE = where 
$NAME = [a-b]+
$OREXPRESSION = $ANDEXPRESSION | $ANDEXPRESSION $ANDOP $OREXPRESSION
$ANDEXPRESSION = $COMPARAISON | $OREXPRESSION | $NOTOP $ANDEXPRESSION
$COMPARAISON = $TERM $COMPOP $TERM 
$COMPOP = > | = 
$ANDOP =  and  
$NOTOP =  not  
$TERM = [0-1]+ | $NAME | $QUOTE $NAME $QUOTE
$QUOTE = "
$HEADER = select c from  
